{
  "name": "@ipincamp/web-scraper",
  "version": "2.0.0",
  "description": "Simple and lightweight web scraper. Works well in 2025!",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "ts-node ./example.ts",
    "build": "tsup ./src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=22.13.1",
    "npm": ">=10.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipincamp/web-scraper.git"
  },
  "keywords": [
    "javascript",
    "nodejs",
    "typescript"
  ],
  "author": "ipincamp",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ipincamp/web-scraper/issues"
  },
  "homepage": "https://github.com/ipincamp/web-scraper#readme",
  "dependencies": {
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "form-data": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
